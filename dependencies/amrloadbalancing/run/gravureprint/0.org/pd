FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location	"0";
	object		pd;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions		[1 -1 -2 0 0 0 0];
internalField	uniform 0;

boundaryField
{
	impellerWall
	{
		type            zeroGradient;
	}

	baffleWall
	{
		type            zeroGradient;
	}

	blade
	{
		type            zeroGradient;
	}

	substrate
	{
		type            zeroGradient;
	}

	insideSlider
	{
		type		ggi;
		value		$internalField;
	}

	outsideSlider
	{
		type		ggi;
		value		$internalField;
	}

	atmosphere
	{
		//type        zeroGradient;
		type		totalPressure;
		p0			uniform 0;
		U			U;
		phi			phi;
		rho			rho;
		psi			none;
		gamma		1;
		value		uniform 0;
	}

	frontAndBack
	{
		type		empty;
	}
}

