FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createPatchDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Tolerance used in matching faces. Absolute tolerance is span of
// face times this factor. To load incorrectly matches meshes set this
// to a higher value.
matchTolerance 1E-3;

// Do a synchronisation of coupled points after creation of any patches.
// Note: this does not work with points that are on multiple coupled patches
//       with transformations.
pointSync true;

// Patches to create.
patchInfo
(
    
    {
        name substrate;
        dictionary
        {
            type patch;
        }
        constructFrom patches;
        patches ( auto0 );
    }

    {
        name atmosphere;
        dictionary
        {
            type patch;
        }
        constructFrom patches;
        patches ( auto1 auto2 auto6 auto12 );
    }

    {
        name baffleWall;
        dictionary
        {
            type patch;
        }
        constructFrom patches;
        patches ( auto3 auto4 auto5 );
    }

    {
        name blade;
        dictionary
        {
            type patch;
        }
        constructFrom patches;
        patches ( auto8 auto9 auto10 auto11 );
    }

    {
        name outsideSlider;
        dictionary
        {
            type patch;
        }
        constructFrom patches;
        patches ( auto7 );
    }

    {
        name frontAndBack;
        dictionary
        {
            type patch;
        }
        constructFrom patches;
        patches ( auto13 auto14 );
    }
);
