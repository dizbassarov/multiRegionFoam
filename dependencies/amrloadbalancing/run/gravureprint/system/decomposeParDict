FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 2;

method          simple;

// Face zones which need to be present on all CPUs in its entirety
globalFaceZones ( insideZone outsideZone );

hierarchicalCoeffs
{
    n               ( 2 1 1 );
    delta           0.001;
    order           xyz;
}

simpleCoeffs
{
    n               (2 1 1);
    delta           0.001;
}

distributed     no;

roots           ( );


