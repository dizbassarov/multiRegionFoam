writeInterval   1e-4;//50;//

cycleWrite      0;

writeFormat     ascii;

writePrecision  16;

writeCompression uncompressed;

timeFormat      general;

timePrecision   12;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           1;

maxDeltaT       1e-5;
minDeltaT       1e-7;

