FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    interfaceShadow
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    interface
    {
        type            fixedGradient;
        gradient        uniform (0 0 0);
    }
    space
    {
        type            inletOutlet;
        inletValue      uniform (0 -1e-06 0);
        value           uniform (0 -1e-06 0);
    }
}


