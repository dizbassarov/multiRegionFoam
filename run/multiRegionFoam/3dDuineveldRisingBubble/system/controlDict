FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs ( "libscalePatchPhi.so" "libinterfaceTrackingFvMesh.so" "libcoupledFvPatchBCs.so" "liblduSolvers.so");
  
applicationClass multiRegionFoam;

startFrom latestTime;

startTime 0;

stopAt endTime;

endTime 0.25;

deltaT 5e-6;

writeControl runTime;

writeInterval 1e-4;

cycleWrite 0; 

writeFormat ascii;

writePrecision 16;

writeCompression uncompressed;

timeFormat general;

timePrecision 12;

runTimeModifiable yes;

adjustTimeStep no;

maxCo 0.3; 

maxDeltaT 1e-05;

minDeltaT 1e-07;

