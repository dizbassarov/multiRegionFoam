FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      coupledSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    // fluidB must be first entry since fluidA requires it for
    // building global patch communication within interfaceTrackingFvMesh
    // (must be also reflected in dynamicMeshDict)
    navierStokesFluid (fluidA fluidB)
);

partitionedTypeRegionInterfaces
(
    // The mesh of the first entry must hold a finite area mesh
    // for motion with interfaceTrackingFvMesh
    fluidAfluidB
    (
        (((fluidA interface) (fluidB interfaceShadow)) ())
    )
);

monolithicTypeRegionInterfaces
();

// Assembles a super-mesh
superMeshRegions
();

partitionedCoupling
{
    maxCoupleIter 6;
}

