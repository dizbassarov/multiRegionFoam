fluidA
{
    // Density 
    rho             rho [1 -3 0 0 0 0 0] 998.3;

    // Viscosity
    mu              mu [ 1 -1 -1 0 0 0 0 ] 1e-3; 
}


fluidB
{
    // Density 
    rho             rho [1 -3 0 0 0 0 0] 1.205;

    // Vicosity
    mu              mu [ 1 -1 -1 0 0 0 0 ] 1.82e-5; 
}

