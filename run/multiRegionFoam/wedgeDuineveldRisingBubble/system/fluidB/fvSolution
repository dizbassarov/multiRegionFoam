FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          BICCG;
        preconditioner  SymGaussSeidel;
        tolerance       1e-12;
        relTol          0;
    }

    pcorr
    {
        solver          BiCGStab;
        preconditioner  DILU;
        tolerance       1e-12;
        relTol          0;
    }

    pFinal
    {
        solver          BICCG;
        preconditioner  DILU;
        tolerance       1e-12;
        relTol          0;
    }

    U
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-6;
        relTol          0;
        minIter         2;
        maxIter         1000;
    }

    cellMotionU
    {
        solver          BICCG;
        preconditioner  DILU;
        tolerance       1e-12;
        relTol          0;
/*        solver          GMRES;*/
/*        nDirections     6;*/
/*        preconditioner  Cholesky;*/
/*        tolerance       1e-15;*/
/*        relTol          0;*/
/*        minIter         2;*/
/*        maxIter         600;*/
    }

}

PISO
{
    pRefPoint (5e-5 0 0); 
    pRefValue 0;
}

PIMPLE
{
    // p-U coupling
    nOuterCorrectors            3; // SIMPLEC loops
    nCorrectors                 2; // PISO loops
    nNonOrthogonalCorrectors    1;

    pRefPoint (5e-4 0 0);
    pRefValue 0;

    residualControl
    {
        p
        {
            relTol         0.01;
            tolerance      0.0001;
        }

        U
        {
            relTol         0.01;
            tolerance      0.0001;
        }
    }
}

relaxationFactors
{}

closedVolume yes;


