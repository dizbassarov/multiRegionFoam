FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/inner";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 283;

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           $internalField; 
    }
    inner_to_solid
    {
/*        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        refValue        uniform 283;
        refGradient     uniform 0;
        valueFraction   uniform 1;
        value           uniform 283;
        Tnbr            T;
        thicknessLayers 0();
        kappaLayers     0();
        kappa           fluidThermo;
        kappaName       none;
*/
        type                    regionCoupledScalarJump;
        k                       "k";
        neighbourRegionName     solid;
        neighbourPatchName      solid_to_inner;
        neighbourFieldName      T;
        secondOrder             false;
        nonOrthCorr             true;
        value                   $internalField;       
    }
    inner_to_domain1
    {
        type            zeroGradient;
    }
}


