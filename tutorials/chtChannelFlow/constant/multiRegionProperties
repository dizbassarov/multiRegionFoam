FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      coupledSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    ( fluid (transportTemperature) )
    ( solid (conductTemperature) )
);

partitionedCoupling
{
    maxCoupleIter 6;
}

DNA
{
    UpPimple
    {
        maxCoupleIter 20;

        residualControl
        {
            U
            {
                maxJumpRes 1e-4;
                outputJumpResField yes;
                maxFluxRes 1e-3;
                outputFluxResField yes;
            }

            p
            {
                maxJumpRes 1e-3;
                outputJumpResField yes;
                maxFluxRes 1e-4;
                outputFluxResField yes;
            }
        }
    }
    T
    {
        maxCoupleIter 20;
    }
}

