FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.1 0 0);

boundaryField
{
    interface
    {
        //type            noSlip;
        //type            fixedValue;
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
        type            slip;
    }
    bottom
    {
        //type            noSlip;
        //type            fixedValue;
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    slip-bottom
    {
        type            slip;
    }
    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //
